{
    "name": "wcmf/wcmf",
    "description": "The wCMF web application framework",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "nusphere/nusoap",
                "version": "0.9.5",
                "dist": {
                    "url": "http://downloads.sourceforge.net/project/nusoap/nusoap/0.9.5/nusoap-0.9.5.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["lib/"]
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0",
        "apache/log4php": "2.3.0",
        "zf1/zend-db": "1.12.6",
        "zf1/zend-http": "1.12.6",
        "zf1/zend-search-lucene": "1.12.6",
        "ircmaxell/password-compat": "1.0.3",
        "sybio/image-workshop": "2.0.5",
        "sybio/gif-frame-extractor": "1.0.0",
        "sybio/gif-creator": "1.0.0",
        "helios-ag/fm-elfinder-php-connector": "1.1.1",
        "nusphere/nusoap": "0.9.5",
        "smarty/smarty": "v3.1.18",
        "itbz/fpdi": "1.4.4"
    },
    "autoload": {
        "psr-4": {
            "wcmf\\": "src/wcmf/"
        }
    }
}